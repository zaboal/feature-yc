{
    "name": "Yandec Cloud CLI",
    "id": "yc",
    "version": "1.0.0",
    "description": "The Yandex Cloud command line interface",
    "dependsOn": {
        "ghcr.io/devcontainers/features/common-utils:2.5.2": {}
    },
    "options": {
        "cli-install-path": {
            "description": "Install to a specific directory.",
            "type": "string",
            "enum": [],
            "default": ""
        },
        "rc-path": {
            "description": "Automatically modify RC_FILE with PATH modification and shell completion.",
            "type": "string",
            "enum": [],
            "default": ""
        },
        "no-rc": {
            "description": "Don't modify the RC file.",
            "type": "string",
            "enum": ["yes", "no"],
            "default": "yes"
        },
        "autp-rc": {
            "description": "Automatically modify default rc file with PATH modification and shell completion.",
            "type": "string",
            "enum": ["yes", "no"],
            "default": "yes"
        }
    },
    "documentationURL": "https://yandex.cloud/docs/cli",
    "keywords": ["aws", "cloud", "cli", "yandex"]
}
